{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": 0,
    "global-require": 0,
    "eslint linebreak-style": [0, "error", "windows"], // 0 = "off"
    "semi": 0,
    "arrow-parens": 0,
    "comma-dangle": 0,
    "require-jsdoc": 0
  },
  "extends": ["eslint:recommended", "google"]
}
